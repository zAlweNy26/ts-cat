		"fastembed": "^1.14.1",
		"get-port-please": "^3.1.2",
		"html-to-text": "^9.0.5",
		"is-docker": "^3.0.0",
		"js-tiktoken": "^1.0.12",
		"langchain": "^0.2.10",
		"lodash": "^4.17.21",
		"lowdb": "^7.0.1",
		"mammoth": "^1.8.0",
		"nodemon": "^3.1.0",
		"ofetch": "^1.3.4",
		"officeparser": "^4.1.1",
		"pdf-parse": "^1.1.1",
